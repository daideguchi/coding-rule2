{
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/**": true,
        "**/.hg/store/**": true
    },
    "workbench.editorAssociations": {
        "STATUS.md": "default"
    },
    "markdown.preview.openMarkdownLinks": "inEditor",
    "files.defaultLanguage": "markdown",
    "workbench.startupEditor": "none"
}
