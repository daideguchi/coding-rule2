{
  "mcpServers": {
    "o3-search": {
      "command": "/opt/homebrew/bin/o3-search-mcp",
      "args": [],
      "env": {
        "OPENAI_API_KEY": "${OPENAI_API_KEY}",
        "SEARCH_CONTEXT_SIZE": "medium",
        "REASONING_EFFORT": "high"
      }
    },
    "github-bridge": {
      "command": "python3",
      "args": [
        "${PROJECT_ROOT}/ai-agents/CLAUDE_MCP_BRIDGE.py"
      ],
      "env": {
        "GITHUB_TOKEN": "${GITHUB_TOKEN}",
        "PROJECT_ROOT": "${PROJECT_ROOT}"
      }
    },
    "tmux-integration": {
      "command": "node",
      "args": [
        "${PROJECT_ROOT}/ai-agents/mcp/tools/tmux_mcp.js"
      ],
      "env": {
        "TMUX_SESSION": "multiagent"
      }
    },
    "filesystem": {
      "command": "python3",
      "args": [
        "${PROJECT_ROOT}/ai-agents/mcp/tools/filesystem_mcp.py"
      ],
      "env": {
        "PROJECT_ROOT": "${PROJECT_ROOT}"
      }
    },
    "gemini-cli": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "@choplin/mcp-gemini-cli",
        "--allow-npx"
      ],
      "env": {}
    }
  }
}